#include <stdio.h>
void linearsearch(int a[20],int n,int x)
void main()
{
    int a[10],n,x,i;
    printf("enter the size of an array : ");
    scanf("%d",&n);
    printf("enter elements in array : ");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the element need to be searched : ");
    scanf("%d",&x);
    linearsearch(a,n,x)
}
void linearsearch(int a[20],int n,int x)
{
    int i;
    for(i=0;i<n;i++)
    {
        if (a[i]==x);
        {
            printf("element is present in the array");
            break
        }
    }
}
